{
    "name": "Switch",
    "description": "A simple redirect extension.",
    "manifest_version": 3,
    "version": "1.0.0",
    "author": "Matheus Alves Pereira - https://github.com/matheus-alpe",
    "permissions": [
        "declarativeNetRequest",
        "declarativeNetRequestFeedback",
        "storage",
        "tabs"
    ],
    "host_permissions": [
        "<all_urls>"
    ],
    "background": {
        "service_worker": "background.js",
        "type": "module"
    },
    "icons": {
        "16": "assets/icon-16x16-deactivated.png",
        "32": "assets/icon-32x32.png",
        "48": "assets/icon-48x48.png",
        "128": "assets/icon-128x128.png"
    },
    "action": {
        "default_icon": {
            "16": "assets/icon-16x16-deactivated.png",
            "32": "assets/icon-32x32.png",
            "48": "assets/icon-48x48.png",
            "128": "assets/icon-128x128.png"
        },
        "default_popup": "index.html",
        "default_title": "Add redirect rules."
    }
}