{
  "name": "chrome-extension-url-switch",
  "description": "Simple redirect extension",
  "private": true,
  "version": "1.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build:backend": "vue-tsc && vite build -c backend/extension.vite.config.ts",
    "build:all": "vue-tsc && vite build && vite build -c backend/extension.vite.config.ts && cd dist && zip -r url-switch .",
    "clear": "rm -rf ./dist"
  },
  "dependencies": {
    "@mdi/font": "^7.4.47",
    "vue": "^3.2.45",
    "vuetify": "^3.5.14"
  },
  "devDependencies": {
    "@types/chrome": "^0.0.217",
    "@types/node": "^18.14.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "sass": "^1.58.3",
    "sass-loader": "^13.2.0",
    "typescript": "^4.9.3",
    "vite": "^4.1.0",
    "vue-tsc": "^1.0.24"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/matheus-alpe/url-switch-extension.git"
  },
  "keywords": [
    "Chrome",
    "Extension",
    "Redirect",
    "Browser"
  ],
  "author": "Matheus Alves Pereira | @matheus-alpe",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/matheus-alpe/url-switch-extension/issues"
  },
  "homepage": "https://github.com/matheus-alpe/url-switch-extension#readme"
}
